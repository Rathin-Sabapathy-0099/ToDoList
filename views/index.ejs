<%- include("header") %>

        <div class="box" id="heading">
            <h1><%=title%></h1>
        </div>

        <div class="box">
            <% for (var i=0;i<list.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%=list[i]%></p>
            </div>
            <%}%>
            <form action="/" class="item" method="POST">
                <input type="text" name="work" placeholder="New Item" autocomplete="off">
                <button type="submit" name="btn" value=<%=title%>>+</button>
            </form> 
        </div>

<%- include("footer") %> 